{
  "name": "dat-library-ui",
  "version": "0.0.5",
  "description": "A number of generic ui elements for Dat Library",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:rollup; npm run build:sample",
    "build:rollup": "cross-env NODE_ENV=production rollup -c",
    "build:sample": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.js",
    "dev": "concurrently -k \"npm run dev:rollup\" \"npm run dev:sample\" -n rollup,webpack",
    "dev:rollup": "rollup -c -w",
    "dev:sample": "webpack-dev-server --inline --host 0.0.0.0 --hot --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/e-e-e/dat-library-ui"
  },
  "author": "Benjamin Forster",
  "dependencies": {
    "lodash": "^4.17.4",
    "tachyons": "^4.8.1",
    "tachyons-sass": "^4.7.4",
    "vue": "^2.4.2",
    "vue-pass-props": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.2.0",
    "babili-webpack-plugin": "^0.1.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.16.1",
    "inject-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.2",
