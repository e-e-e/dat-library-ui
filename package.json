    "sass-loader": "^6.0.6",
    "spectron": "^3.7.1",
    "style-loader": "^0.18.1",
    "svgo": "^0.7.2",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^12.2.1",
